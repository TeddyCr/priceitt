{
    "schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Token",
    "description": "A token of the system",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "description": "The ID of the token"
        },
        "name": {
            "type": "string",
            "description": "The name of the token"
        },
        "description": {
            "type": "string",
            "description": "The description of the token"
        },
        "displayName": {
            "type": "string",
            "description": "The display name of the token"
        },
        "updatedAt": {
            "type": "string",
            "format": "date-time",
            "description": "The date and time the token was updated"
        },
        "createdAt": {
            "type": "string",
            "format": "date-time",
            "description": "The date and time the token was created"
        },
        "token": {
            "type": "string",
            "description": "The token"
        },
        "tokenType": {
            "type": "string",
            "description": "The type of the token"
        },
        "expirationDate": {
            "type": "string",
            "format": "date-time",
            "description": "The date and time the token will expire"
        },
        "userId": {
            "type": "string",
            "description": "The ID of the user"
        },
        "deviceId": {
            "type": "string",
            "description": "The ID of the device"
        },
        "ip": {
            "type": "string",
            "description": "The IP address of the device"
        }
    },
    "additionalProperties": false,
    "required": ["id", "name", "token", "tokenType", "expirationDate", "userId"]
}